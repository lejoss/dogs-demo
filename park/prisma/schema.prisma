generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model dog {
  id     Int     @id
  name   String
  age    Int?
  breed  String?
  size   String?
  gender String?
  iduser Int?
}

model park {
  id   Int    @id
  name String
}

model user {
  id           Int    @id @default(autoincrement())
  subscription String
}

model visit {
  id     Int       @id
  user   String
  date   DateTime? @db.Date
  status Boolean?  @default(false)
}
