generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model dog {
  id       Int       @id @default(autoincrement())
  name     String
  age      Int?
  breed    String?
  size     String?
  gender   String?
  active   Boolean?  @default(false)
  lastseen DateTime? @db.Date
  userid   Int       @unique
  user     user      @relation(fields: [userid], references: [id])
}

model park {
  id   Int    @id
  name String
}

model user {
  id           Int     @id @default(autoincrement())
  subscription String
  endpoint     String? @unique
  dog          dog?
}
